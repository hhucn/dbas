          <button id="popup-split-statement-add" class="pull-left btn btn-secondary" type="button">
            <i class="fa fa-plus" aria-hidden="true"></i>
          </button>
          <button id="popup-split-statement-rem" class="pull-left btn btn-secondary" type="button">
            <i class="fa fa-minus" aria-hidden="true"></i>
          </button>
          <button type="button" class="btn btn-secondary" id="popup-split-statement-btn-yes" i18n:translate="submit">
            Submit
          </button>
          <button type="button" class="btn btn-danger" data-dismiss="modal" i18n:translate="cancel">Cancel</button>
        </div>
      </div>
    </div>
  </div>

