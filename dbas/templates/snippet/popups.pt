        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-danger" id="popup-delete-content-submit">Okay</button>
          <button type="button" class="btn btn-success"
                  id="popup-delete-content-close">${structure:extras.tag.close}</button>
        </div>
      </div>
    </div>
