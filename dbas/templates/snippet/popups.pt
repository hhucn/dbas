    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button name="popup-merge-premisegroup-close" type="button" class="close" data-dismiss="modal"
                  aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h4 class="modal-title" id="myModalLabelMergePremisegroup" i18n:translate="merge_statement">
            Merge Statement
          </h4>
        </div>
        <div class="modal-body">
          <p class="lead" i18n:translate="merge_multiple_statements_of_pgroup">
