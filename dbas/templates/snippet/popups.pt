        </div>
        <div class="modal-body">
          <p class="lead">${extras.title.select_multiple_statements}</p>
          <div id="popup-choose-statement-radios"></div>
        </div>
      </div>
    </div>
  </div>

  <!-- SPLIT STATEMENT -->
  <div class="modal fade" id="popup-split-statement" tabindex="-1" role="dialog"
