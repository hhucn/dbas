          <div class="col-md-12 list-group" id="proposal-duplicate-list-group"></div>
        </div>

        <div class="modal-footer">
          <button id="popup-flag-statement-accept-btn" type="button"
                  class="btn btn-success btn-secondary">${structure:extras.tag.submit}</button>
        </div>
      </div>
    </div>
