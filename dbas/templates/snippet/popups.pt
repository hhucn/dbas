<div i18n:domain="dbas" metal:define-macro="layout">

  <!-- EDIT STATEMENTS -->
  <div class="modal fade" id="popup-edit-statement" tabindex="-1" role="dialog"
       aria-labelledby="myModalLabelEditStatements" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" id="popup-edit-statement-close" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h5 class="modal-title" id="myModalLabelEditStatements">${extras.title.edit_statement}</h5>
        </div>
        <div id="popup-edit-statement-body">
          <p id="popup-edit-statement-success-description" class="text-success"></p>
          <p id="popup-edit-statement-error-description" class="text-danger"></p>
          <p id="popup-edit-statement-info-description" class="text-info"></p>
          <p class="lead">${structure:extras.tag.edit_statement_here}</p>
          <div id="popup-edit-statement-inputs"></div>
          <div class="list-group" id="proposal-edit-list-group"></div>
          <p class="text-right">
            <small><a href="#" id="popup-edit-statement-changelog-view"></a></small>
          </p>
          <p class="text-right">
            <small><a href="#" id="popup-edit-statement-changelog-hide"></a></small>
          </p>
          <p class="lead">${extras.title.view_changelog}:</p>
          <div id="popup-edit-statement-logfile"></div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-success"
                  id="popup-edit-statement-submit">${structure:extras.tag.submit}</button>
          <button type="button" class="btn btn-danger"
                  id="popup-edit-statement-close-button">${structure:extras.tag.close}</button>
        </div>
      </div>
    </div>
  </div>

  <!-- CONFIRM DIALOG POPUP -->
  <div class="modal fade" id="confirm-dialog-popup" tabindex="-1" role="dialog"
       aria-labelledby="myModalLabelConfirmDialog" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">
            <span id="myModalLabelConfirmDialog">...</span>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </h4>
        </div>
        <div class="modal-body">
        </div>
        <div class="modal-footer">
          <button id="confirm-dialog-accept-btn" type="button"
                  class="btn btn-success btn-secondary" i18n:translate="okay">Okay
          </button>
          <button id="confirm-dialog-refuse-btn" type="button"
                  class="btn btn-danger btn-secondary" i18n:translate="cancel">Cancel
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- CONFIRM ROW DIALOG POPUP -->
  <div class="modal fade" id="confirm-row-dialog-popup" tabindex="-1" role="dialog"
       aria-labelledby="myModalLabelConfirmRowDialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h4 class="modal-title" id="myModalLabelConfirmRowDialog">...</h4>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col-md-6"></div>
            <div class="col-md-offset-1 col-md-5"></div>
          </div>
        </div>
        <div class="modal-footer">
          <button id="confirm-row-dialog-accept-btn" type="button"
                  class="btn btn-success btn-secondary" i18n:translate="okay">
            Okay
          </button>
          <button id="confirm-row-dialog-refuse-btn" type="button"
                  class="btn btn-danger btn-secondary" i18n:translate="cancel">
            Cancel
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- CONFIRM DIALOG POPUP WITH CHECKBOX -->
  <div class="modal fade" id="confirm-dialog-checkbox-popup" tabindex="-1" role="dialog"
       aria-labelledby="myModalLabelConfirmDialogCheckbox" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h4 class="modal-title" id="myModalLabelConfirmDialogCheckbox">...</h4>
        </div>
        <div class="modal-body"></div>
        <div id="modal-checkbox-body">
          <label id="confirm-dialog-checkbox-label" for="confirm-dialog-checkbox">
            <input id="confirm-dialog-checkbox" type="checkbox">&thinsp;&thinsp;
            <span id="confirm-dialog-checkbox-text" i18n:translate="do_not_remind_me_again">
              Do not remind me again
            </span>
          </label>
        </div>
        <div class="modal-footer">
          <button id="confirm-dialog-checkbox-accept-btn" type="button"
                  class="btn btn-success btn-secondary" i18n:translate="okay">
            Okay
          </button>
          <button id="confirm-dialog-checkbox-refuse-btn" type="button"
                  class="btn btn-danger btn-secondary" i18n:translate="cancel">
            Cancel
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- URL SHARING POPUP -->
  <div class="modal fade" id="popup-url-sharing" tabindex="-1" role="dialog" aria-labelledby="myModalLabelURL"
       aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" id="popup-url-sharing-close" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h5 class="modal-title" id="myModalLabelURL">${extras.tag.url_sharing}</h5>
        </div>
        <div id="popup-url-sharing-body">
          <div id="popup-url-sharing-content"></div>
          <p id="popup-url-sharing-description-p">${extras.tag.url_sharing_description}</p>
          <div class="form-group">
            <div class="col-sm-12 input-group">
              <div class="input-group-addon"><i class="fa fa-share fa-2x" aria-hidden="true"></i></div>
              <input id="popup-url-sharing-input" class="form-control" type="text" placeholder="URL">
            </div>
          </div>

        </div>
        <div class="modal-footer">
          <div class="pull-left">
            <a class="social-wrapper share-mail" id="share-url-mail" data-toggle="tooltip" data-placement="bottom"
               title="Mail">
              <i class="social-em fa fa-envelope social fa-2x"></i></a>
            <a class="social-wrapper share-twitter" id="share-url-twitter" data-toggle="tooltip" data-placement="bottom"
               title="Twitter"><i class="social-tw fa fa-twitter social fa-2x"></i></a>
            <a class="social-wrapper share-google" id="share-url-google" data-toggle="tooltip" data-placement="bottom"
               title="Google+"><i class="social-gp fa fa-google-plus social fa-2x"></i></a>
            <a class="social-wrapper share-facebook" id="share-url-facebook" data-toggle="tooltip"
               data-placement="bottom" title="Facebook"><i class="social-fb fa fa-facebook social fa-2x"></i></a>
          </div>
          <button type="button" class="btn btn-success" id="popup-url-sharing-long-url-button"
                  data-is-short-url="1">${structure:extras.tag.fetchurl}</button>
          <button type="button" class="btn btn-secondary"
                  id="popup-url-sharing-close-button">${structure:extras.tag.close}</button>
        </div>
      </div>
    </div>
  </div>

  <!-- COMPLETE LOGIN INFORMATION  POPUP -->
  <div class="modal fade" id="popup-complete-login" tabindex="-1" role="dialog" aria-labelledby="myModalLabelURL"
       aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" id="popup-complete-login-close" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h5 class="modal-title" i18n:translate="information_missing">Information are missing</h5>
        </div>
        <div id="popup-complete-login-body">
          <p class="text-info" style="margin: 0 2em;" i18n:translate="plase_add_missing_infos_we_add_account">During the
            authentication we could not fetch all of the necessary information. Please add the missing information, so
            we can add a new account in D-BAS (Login via other providers won't work).</p>
          <form autocomplete="on" style="margin: 2em;">
            <div class="form-group">
              <label class="col-md-3 control-label"
                     for="popup-complete-login-userfirstname-input" i18n:translate="first_name">Your First Name</label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-user fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-userfirstname-input" type="text" name="firstname"
                       class="form-control userfirstname active" autocomplete="on"
                       placeholder="${extras.tag.placeholder_firstname}">
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label"
                     for="popup-complete-login-userlastname-input" i18n:translate="last_name">Your Last Name</label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-user fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-userlastname-input" type="text" name="surname"
                       class="form-control userlastname" autocomplete="on"
                       placeholder="${extras.tag.placeholder_lastname}">
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label" for="popup-complete-login-nick-input" i18n:translate="nickname">
                Your Nickname
              </label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-user-secret fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-nick-input" type="text" class="form-control nickname" name="nickname"
                       autocomplete="off" placeholder="${extras.tag.placeholder_nickname}">
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label" i18n:translate="your_gender">Your Gender</label>
              <div class="col-md-9 input-group" style="padding-left: 1.3em">
                <input class="radio-inline" type="radio" name="inlineRadioGenderOptions"
                       id="popup-complete-login-inlineRadioGender1" value="-" checked>
                <label for="popup-complete-login-inlineRadioGender1">
                  <i class="fa fa-times fa-2x" aria-hidden="true"></i>
                </label>
                <input class="radio-inline" type="radio" name="inlineRadioGenderOptions"
                       id="popup-complete-login-inlineRadioGender2" value="f">
                <label for="popup-complete-login-inlineRadioGender2">
                  <i class="fa fa-female fa-2x" aria-hidden="true"></i>
                </label>
                <input class="radio-inline" type="radio" name="inlineRadioGenderOptions"
                       id="popup-complete-login-inlineRadioGender3" value="m">
                <label for="popup-complete-login-inlineRadioGender3">
                  <i class="fa fa-male fa-2x" aria-hidden="true"></i>
                </label>
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label" for="popup-complete-login-email-input" i18n:translate="your_email">
                Your Email Address</label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-envelope fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-email-input" type="email" class="form-control email" name="email"
                       autocomplete="on" placeholder="${extras.tag.placeholder_mail}">
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label" for="popup-complete-login-password-input" i18n:translate="password">
                Your Password</label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-lock fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-password-input" type="password" name="password"
                       class="form-control password" autocomplete="off"
                       placeholder="${extras.tag.placeholder_password}">
              </div>
            </div>
            <div class="form-group">
              <label class="col-md-3 control-label"
                     for="popup-complete-login-passwordconfirm-input" i18n:translate="password-confirm">
                Password (confirm)</label>
              <div class="col-md-9 input-group">
                <div class="input-group-addon"><i class="fa fa-unlock fa-2x" aria-hidden="true"></i></div>
                <input id="popup-complete-login-passwordconfirm-input" type="password" name="passwordconfirm"
                       class="form-control password" autocomplete="off"
                       placeholder="${extras.tag.placeholder_password}">
              </div>
            </div>
          </form>

          <div class="col-md-12" id="popup-complete-login-failed" style="display: none;">
            <div class="alert alert-danger" role="alert">
              <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
              <span class="sr-only" i18n:translate="error">Error:</span>
              <span id="popup-complete-login-failed-message"></span>
            </div>
          </div>

          <div class="col-md-12" id="popup-complete-login-info" style="display: none;">
            <div class="alert alert-warning" role="alert">
              <i class="fa fa-info-circle" aria-hidden="true"></i>
              <span class="sr-only" i18n:translate="info">Info:</span>
              <span id="popup-complete-login-info-message"></span>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button id="popup-complete-login-register-button" type="button"
                  class="btn btn-primary" i18n:translate="register">
            Register
          </button>
          <button id="popup-complete-login-close-button" type="button" class="btn btn-secondary" i18n:translate="close"
                  onclick="$('#popup-complete-login').modal('hide');">
            Close
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- LOGIN POPUP -->
  <div class="modal fade" id="popup-login" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLogin"
       aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" id="popup-login-close" class="close" data-dismiss="modal" aria-label="Close"><span
              aria-hidden="true">&times;</span>
          </button>
          <br>
          <ul class="nav nav-tabs" style="display: table; margin: 0 auto;">
            <li id="nav-tab-login" class="tab-login active"><a href="#login" i18n:translate="login">Log In</a></li>
            <!--<li id="nav-tab-signup" class="tab-login"><a href="#signup" i18n:translate="sign_up">Sign Up</a></li>-->
          </ul>
        </div>
        <div id="popup-login-body">
          <div class="alert alert-info">
            <p i18n:translate="all_data_is_free">Your submitted data is being licensed under Creative Commons
              Share Alike and will be used for scientific purposes. We will never share your
              password or e-mail address. By registering or logging in, you are accepting these terms.</p>
            (<a href="https://creativecommons.org/licenses/by-sa/4.0/">CC BY-SA 4.0</a>)
          </div>
          <div class="alert alert-info">
            <p>
              Für diese Experiment ist der Login nur mit Ihrer Unikennung (z.B. bjebb100) möglich!
              Nur Studierende der Informatik können sich einloggen. Sollten Sie sich nicht trotzdem nicht einloggen
              können, melden Sie sich bitte bei uns.
            </p>
          </div>
          <div class="tab-content">
            <div id="login">
              <form autocomplete="on">
                <div class="form-group">
                  <div class="col-md-12">
                    <label class="col-md-3 control-label" for="login-user" i18n:translate="your_nickname">
                      Your Nickname
                    </label>
                    <div class="col-md-9 input-group">
                      <div class="input-group-addon"><i class="fa fa-user fa-2x" aria-hidden="true"></i></div>
                      <input id="login-user" type="text" class="form-control"
                             placeholder="${extras.tag.placeholder_nickname}" autocomplete="on">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-md-12">
                    <label class="col-md-3 control-label" for="login-pw" i18n:translate="your_password">
                      Your Password
                    </label>
                    <div class="col-md-9 input-group">
                      <div class="input-group-addon"><i class="fa fa-lock fa-2x" aria-hidden="true"></i></div>
                      <input id="login-pw" type="password" class="form-control"
                             placeholder="${extras.tag.placeholder_password}" autocomplete="on">
                    </div>
                  </div>
                </div>
              </form>
              <div style="padding-left: 15px;">
                <label>
                  <input id="keep-login-box" type="checkbox" style="float:left;">
                  <span i18n:translate="keep_me_logged_in">Keep me logged in</span>
                </label>
                <a id="popup-login-forgot-password-text"
                   class="cursor-pointer pull-right" i18n:translate="forgot_password">
                  Forgot your password?
                </a>
              </div>
              <div id="popup-login-forgot-password-body" class="form-group" style="margin-top: 1em;">
                <label for="mail-address" class="control-label" i18n:translate="fill_in_mail_for_password">
                  Please fill in your e-mail below. A new password will be send this address. This does not affect other
                  services of the HHU:
                </label>
                <div class="form-group">
                  <div class="col-md-12 input-group">
                    <div class="input-group-addon"><i class="fa fa-envelope fa-2x" aria-hidden="true"></i></div>
                    <input id="password-request-email-input" type="email" class="form-control"
                           placeholder="${extras.tag.placeholder_mail}" autocomplete="on">
                  </div>
                </div>
                <input id="popup-login-button-request" type="submit" onclick="$('body').addClass('loading')"
                       name="form.passwordrequest.submitted" value="${extras.buttons.request_password}"
                       class="btn btn-primary">
              </div>
              <div class="pull-right">
                <button id="popup-login-button-login" type="button" class="btn btn-primary" i18n:translate="login">
                  Log In
                </button>
                <button type="button" class="btn btn-secondary" id="popup-login-close-button2" i18n:translate="close">
                  Close
                </button>
              </div>
            </div>
            <div id="signup">

              <div id="popup-login-registration-field">
                <div class="form-group">
                  <label class="col-md-3 control-label" for="userfirstname-input" i18n:translate="first_name">
                    Your First Name</label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-user fa-2x" aria-hidden="true"></i></div>
                    <input id="userfirstname-input" type="text" name="firstname"
                           class="form-control userfirstname active" autocomplete="on"
                           placeholder="${extras.tag.placeholder_firstname}">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" for="userlastname-input" i18n:translate="last_name">
                    Your Last Name</label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-user fa-2x" aria-hidden="true"></i></div>
                    <input id="userlastname-input" type="text" name="surname" class="form-control userlastname"
                           autocomplete="on" placeholder="${extras.tag.placeholder_lastname}">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" for="nick-input" i18n:translate="nickname">Your Nickname</label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-user-secret fa-2x" aria-hidden="true"></i></div>
                    <input id="nick-input" type="text" class="form-control nickname" name="nickname" autocomplete="off"
                           placeholder="${extras.tag.placeholder_nickname}">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" i18n:translate="your_gender">Your Gender</label>
                  <div class="col-md-9 input-group" style="padding-left: 1.3em">
                    <input class="radio-inline" type="radio" name="inlineRadioGenderOptions" id="inlineRadioGender1"
                           value="-" checked>
                    <label for="inlineRadioGender1">
                      <i class="fa fa-times fa-2x" aria-hidden="true"></i>
                    </label>
                    <input class="radio-inline" type="radio" name="inlineRadioGenderOptions" id="inlineRadioGender2"
                           value="f">
                    <label for="inlineRadioGender2">
                      <i class="fa fa-female fa-2x" aria-hidden="true"></i>
                    </label>
                    <input class="radio-inline" type="radio" name="inlineRadioGenderOptions" id="inlineRadioGender3"
                           value="m">
                    <label for="inlineRadioGender3">
                      <i class="fa fa-male fa-2x" aria-hidden="true"></i>
                    </label>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" for="email-input" i18n:translate="your_email">
                    Your Email Address
                  </label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-envelope fa-2x" aria-hidden="true"></i></div>
                    <input id="email-input" type="email" class="form-control email" name="email" autocomplete="on"
                           placeholder="${extras.tag.placeholder_mail}">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" for="popup-login-password-input" i18n:translate="password">
                    Your Password
                  </label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-lock fa-2x" aria-hidden="true"></i></div>
                    <input id="popup-login-password-input" type="password" name="password" class="form-control password"
                           autocomplete="off" placeholder="${extras.tag.placeholder_password}">
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label"
                         for="popup-login-passwordconfirm-input" i18n:translate="password-confirm">
                    Password (confirm)
                  </label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon"><i class="fa fa-unlock fa-2x" aria-hidden="true"></i></div>
                    <input id="popup-login-passwordconfirm-input" type="password" name="passwordconfirm"
                           class="form-control password" autocomplete="off"
                           placeholder="${extras.tag.placeholder_password}">
                  </div>
                </div>
                <div class="form-group" style="margin-bottom: 0">
                  <label class="col-md-3 control-label" i18n:translate="password-strength">Password Strength</label>
                  <div class="col-md-9 input-group">
                    <div class="col-md-9 veryweak" id="popup-login-password-meter"></div>
                    <span id="popup-login-password-strength" i18n:translate="password_strength">very weak</span>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-md-3 control-label" for="captcha-answer" i18n:translate="captcha">Solve
                    this captcha</label>
                  <div class="col-md-9 input-group">
                    <div class="input-group-addon">
                      <span id="captcha-digit1"></span> +
                      <span id="captcha-digit2"></span> -
                      <span id="captcha-digit3"></span> =
                    </div>
                    <input id="captcha-answer" type="number" name="captcha-answer" class="form-control"
                           autocomplete="off">
                  </div>
                </div>
                <div class="col-md-12" id="popup-login-registration-success" role="alert">
                  <div class="alert alert-success alert-dismissible">
                    <i class="fa fa-check-circle" aria-hidden="true"></i>
                    <span class="sr-only" i18n:translate="success">Success:</span>
                    <span id="popup-login-registration-success-message"></span>
                  </div>
                </div>
                <div class="col-md-12" id="popup-login-registration-failed" role="alert">
                  <div class="alert alert-danger alert-dismissible">
                    <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
                    <span class="sr-only" i18n:translate="error">Error:</span>
                    <span id="popup-login-registration-failed-message"></span>
                  </div>
                </div>
                <div class="col-md-12" id="popup-login-registration-info" role="alert">
                  <div class="alert alert-warning alert-dismissible">
                    <i class="fa fa-info-circle" aria-hidden="true"></i>
                    <span class="sr-only" i18n:translate="info">Info:</span>
